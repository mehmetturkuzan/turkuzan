public class Solution {
public string[] FindRestaurant(string[] list1, string[] list2) {
int s1=list1.Length,s2=list2.Length;
if(s1==1)
return list1;
if(s2==1)
return list2;

    Dictionary<string,int> sl=new Dictionary<string,int>();
    List<string> rs=new List<string>();
    
    for(int i=0;i<s1;i++)
        sl.Add(list1[i],i);

    int m=int.MaxValue;
    for(int i=0;i<s2;i++)
    {
        if(i>m)
            break;

        if(sl.ContainsKey(list2[i]))
        {
            if(i+sl[list2[i]]<m)
            {
                if(rs.Count>0)
                    rs.Clear();
                m=i+sl[list2[i]];
                rs.Add(list2[i]);
            }else{
                if(i+sl[list2[i]]==m)
                {
                    rs.Add(list2[i]);
                }
            }
        }

    }

    return rs.ToArray();

}

}
